{
  "name": "MYC",
  "version": "1.0.14",
  "description": "My Youtube Companion",
  "manifest_version": 3,
  "background": {
    "service_worker": ["background.js"]
  },
  "author": "kllarena07",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ],
  "permissions": ["tabs"]
}
